pipeline {
    agent any
    
    tools {
        maven 'mymaven'
    }

    stages {
        stage('Code checkout') {
            steps {
                 git branch: 'main', changelog: false, poll: false, url: 'https://github.com/michaelezenweke/edureka-abctech.git'
            }
        }
        stage('Code Compile') {
            steps {
                sh "mvn compile"
            }
        }
        stage('Unit Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Code Packaging') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
